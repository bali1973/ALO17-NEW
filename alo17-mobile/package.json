{
  "name": "alo17-mobile",
  "version": "1.0.0",
  "description": "Alo17 Marketplace Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace Alo17Mobile.xcworkspace -scheme Alo17Mobile -configuration Release archive -archivePath Alo17Mobile.xcarchive",
    "performance:test": "react-native run-android --variant=release && npx react-native-performance-test",
    "performance:monitor": "npx react-native-performance-monitor",
    "performance:profile": "react-native run-android --variant=release --profile",
    "performance:analyze": "npx react-native-bundle-analyzer",
    "performance:memory": "npx react-native-memory-profiler",
    "performance:network": "npx react-native-network-profiler",
    "performance:all": "npm run performance:test && npm run performance:monitor && npm run performance:analyze"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.5.4",
    "react-native-vector-icons": "^10.0.2",
    "react-native-image-picker": "^5.6.0",
    "react-native-camera": "^4.2.1",
    "react-native-maps": "^1.7.1",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-keychain": "^8.1.3",
    "react-native-async-storage": "^1.19.5",
    "react-native-netinfo": "^9.4.1",
    "react-native-device-info": "^10.11.0",
    "react-native-permissions": "^3.10.1",
    "react-native-share": "^9.4.1",
    "react-native-fs": "^2.20.0",
    "react-native-image-crop-picker": "^0.40.2",
    "react-native-modal": "^13.0.1",
    "react-native-super-grid": "^4.8.1",
    "react-native-paper": "^5.11.1",
    "react-native-elements": "^3.4.3",
    "axios": "^1.5.0",
    "socket.io-client": "^4.7.2",
    "react-native-sound": "^0.11.2",
    "react-native-video": "^5.2.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-shadow-2": "^6.2.0",
    "react-native-svg": "^13.14.0",
    "react-native-svg-transformer": "^1.1.0",
    "react-native-credit-card-input": "^0.4.1",
    "react-native-stripe-sdk": "^0.0.19",
    "react-native-paypal": "^1.0.0",
    "react-native-fabric": "^0.5.1",
    "react-native-crashlytics": "^3.3.7",
    "react-native-firebase": "^18.7.3",
    "react-native-google-analytics-bridge": "^7.3.1",
    "react-native-mixpanel": "^1.2.0",
    "react-native-sentry": "^5.17.0",
    "react-native-code-push": "^8.1.0",
    "react-native-config": "^1.5.1",
    "react-native-dotenv": "^3.4.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "keywords": [
    "react-native",
    "marketplace",
    "mobile",
    "ecommerce",
    "alo17"
  ],
  "author": "Alo17 Team",
  "license": "MIT"
} 