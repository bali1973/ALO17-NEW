{
  "name": "alo17",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3004",
    "dev:watch": "nodemon --watch src --watch server.js --exec \"next dev -p 3004\"",
    "build": "next build",
    "build:netlify": "next build && npm run copy-build-files",
    "copy-build-files": "xcopy .next\\static public\\static /E /I /Y && xcopy .next\\server\\app public\\app /E /I /Y && xcopy .next\\server\\app\\*.html public\\ /Y",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "seed": "tsx prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "export": "next export"
  },
