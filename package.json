{
  "name": "alo17",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3004",
    "dev:watch": "nodemon --watch src --watch server.js --exec \"next dev -p 3004\"",
    "build": "next build",
    "start": "next start -p ${PORT:-3004}",
    "lint": "next lint",
    "seed": "tsx prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "jest --testPathPatterns=Performance.test.tsx",
    "test:api": "jest --testPathPatterns=API.test.tsx",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:report": "npm run test:coverage && open coverage/lcov-report/index.html",
    "test:lint": "npm run lint && npm run test",
    "test:type": "npm run type-check && npm run test",
    "test:full": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:dev": "cross-env ANALYZE=true npm run dev",
    "performance:test": "node scripts/performance-test.js",
    "performance:lighthouse": "npx lighthouse http://localhost:3004 --output=json --output-path=./lighthouse-report.json --chrome-flags=\"--headless\"",
    "performance:load": "npx autocannon -c 10 -d 10 http://localhost:3004",
    "deploy": "bash deploy-production.sh",
    "monitoring:test": "npm run test:performance && npm run test:api",
    "security:check": "npm audit --audit-level moderate",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "@swc/helpers": "^0.5.17",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/three": "^0.178.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.0.1",
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "crypto-js": "^4.2.0",
    "eslint": "^8",
    "eslint-config-next": "15.2.2",
    "ethers": "^6.15.0",
    "expo-server-sdk": "^3.15.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.513.0",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "recharts": "^3.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.3.0",
    "swiper": "^11.2.8",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.178.0",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "uuid": "^11.1.0",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.12.0"
  }
}
